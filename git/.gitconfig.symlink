[include]
	path = .dotfiles/secret/.gitconfig
[core]
	excludesfile = ~/.gitignore
[alias]
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
	ui = true
[push]
	default = current
	autoSetupRemote = true
[branch "master"]
	remote = origin
	merge = refs/heads/master
[diff]
	submodule = log
[difftool]
	prompt = false
[status]
	submoduleSummary = true
[submodule]
	recurse = true
[fetch]
	prune = true
[http]
	sslVerify = true
[pull]
	ff = only
[init]
	defaultBranch = master
