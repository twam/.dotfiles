[include]
	path = .dotfiles/secret/.gitconfig
[core]
	excludesfile = ~/.gitignore
[alias]
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
	ui = true
[push]
	default = current
[branch "master"]
	remote = origin
	merge = refs/heads/master
[diff]
	submodule = log
[difftool]
	prompt = false
[status]
	submoduleSummary = true
